[Unit]
Description=SESE Website API Server

[Service]
ExecStart=/usr/bin/stack exec sese-website-exe
WorkingDirectory=/home/sese-website/code/server

Restart=always
RestartSec=5

User=sese-website
Group=sese-website

EnvironmentFile=/home/sese-website/website-env

[Install]
WantedBy=multi-user.target

